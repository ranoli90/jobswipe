{
  "dashboard": {
    "id": null,
    "title": "JobSwipe Overview",
    "tags": ["jobswipe", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Requests per Second",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Job Matching Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(job_matching_requests_total[5m])",
            "legendFormat": "Matching requests"
          },
          {
            "expr": "histogram_quantile(0.95, rate(job_matching_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile duration"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Application Tasks",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(application_tasks_total[5m])",
            "legendFormat": "{{status}} {{ats_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "active_connections",
            "legendFormat": "Active connections"
          },
          {
            "expr": "database_connections_active",
            "legendFormat": "DB connections"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Job Ingestion",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(jobs_ingested_total[5m])",
            "legendFormat": "{{source}} {{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}